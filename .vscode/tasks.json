{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: clang build active file",
            "command": "/usr/bin/clang",
            "args": [
                "-O3",
                "-Wall",
                "-shared",
                "-std=c++14",
                "-fPIC",
                "-fcolor-diagnostics",
                "-fansi-escape-codes",
                "-I/opt/homebrew/opt/python@3.14/Frameworks/Python.framework/Versions/3.14/include/python3.14",
                "-I/opt/homebrew/lib/python3.14/site-packages/pybind11/include",
                "-I/opt/homebrew/include/eigen3",
                "${file}",
                "${workspaceFolder}/src/linreg.cpp",
                "-o",
                "${workspaceFolder}/_core.cpython-314-darwin.so",
                "-undefined",
                "dynamic_lookup"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}